{
  "version": 2,
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "npm run build"
      }
    }
  ],
  "routes": [
    { "src": "/admin/videos", "dest": "/index.html" },
    { "src": "/assets/(.*)\\.js", "headers": { "Content-Type": "text/javascript" } },
    { "src": "/assets/(.*)\\.css", "headers": { "Content-Type": "text/css" } },
    { "src": "/.*", "dest": "/index.html" }
  ]
}
